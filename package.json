{
  "name": "last-commit",
  "description": "Save / read last git commit id from file or from git log",
  "version": "0.0.0-semantic-release",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": {
    "last-commit": "./index.js"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/last-commit/issues"
  },
  "contributors": [],
  "dependencies": {
    "check-more-types": "2.23.0",
    "ggit": "0.13.0",
    "lazy-ass": "1.5.0"
  },
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-nice-package": "0.9.2",
    "grunty": "0.1.2",
    "pre-git": "3.12.0",
    "semantic-release": "4.3.5"
  },
  "engines": {
    "node": "> 0.10.0"
  },
  "homepage": "https://github.com/bahmutov/last-commit",
  "keywords": [
    "git",
    "commit",
    "id",
    "build",
    "ci",
    "utility"
  ],
  "license": "MIT",
  "main": "index.js",
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm prune",
        "npm run nice-package",
        "npm test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/last-commit.git"
  },
  "scripts": {
    "test": "node index.js",
    "nice-package": "grunty grunt-nice-package nice-package",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
