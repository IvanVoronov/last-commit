{
  "name": "last-commit",
  "description": "Save / read last git commit id from file or from git log",
  "version": "0.0.0-semantic-release",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bin": {
    "last-commit": "./index.js"
  },
  "bugs": {
    "url": "https://github.com/bahmutov/last-commit/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm prune",
        "npm run nice-package",
        "npm test"
      ]
    },
    "next-update": {
      "skip": [
        "grunt"
      ]
    }
  },
  "contributors": [],
  "dependencies": {
    "check-more-types": "2.24.0",
    "ggit": "1.23.0",
    "lazy-ass": "1.6.0"
  },
  "devDependencies": {
    "git-issues": "^1.3.1",
    "github-post-release": "1.12.1",
    "grunt": "0.4.5",
    "grunt-nice-package": "0.10.4",
    "grunty": "0.3.0",
    "pre-git": "3.15.0",
    "semantic-release": "6.3.6"
  },
  "engines": {
    "node": "> 0.10.0"
  },
  "homepage": "https://github.com/bahmutov/last-commit",
  "keywords": [
    "build",
    "ci",
    "commit",
    "git",
    "id",
    "utility"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/last-commit.git"
  },
  "scripts": {
    "nice-package": "grunty grunt-nice-package nice-package",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "node index.js",
    "issues": "git-issues"
  },
  "release": {
    "generateNotes": "github-post-release"
  }
}
